#include <stdio.h>
#include <unistd.h>   

int main(void)
{
	void print_alphabet(void)
	{
		char letters[] = "abcdefghijklmnopqrstuvwxyz";                                                                                        
        	int i = 0;                                                                                                                            
                                                                                                                                              
        	while(letters[i] !='\0')                                                                                                              
                {                                                                                                                             
                  putchar(letters[i]);                                                                                                        
                  i++;                                                                                                                        
                }                                                                                                                             
                                                                                                                                              
        	putchar('\n'); 
	}


	print_alphabet(void);
	return(0);
}